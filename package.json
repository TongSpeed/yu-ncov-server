{
  "name": "yu-ncov-server",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "author": "fastspeeed@gmail.com",
  "scripts": {
    "start": "nodemon lib/server",
    "clean": "rm -rf lib",
    "build": "tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only server/schema",
    "postinstall": "npm -s run generate",
    "dxy": "ts-node ./server/data/bin.ts",
    "update": "ts-node ./server/data/updateY.ts",
    "now": "now",
    "migrate": "npm -s run migrate:create && npm -s run migrate:save",
    "migrate:create": "prisma2 migrate save --name 'add-comment-model' --experimental",
    "migrate:save": " prisma2 migrate up --experimental",   
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly server/server"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-preview021",
    "@types/node-fetch": "2.5.4",
    "apollo-server": "^2.10.0",
    "apollo-server-micro": "^2.10.0",
    "bcryptjs": "2.4.3",
    "date-fns": "^2.9.0",
    "fp-ts": "^2.4.2",
    "graphql": "14.6.0",
    "jsonwebtoken": "8.5.1",
    "macoolka-log": "^0.2.0",
    "macoolka-predicate": "^0.2.1",
    "nexus": "0.12.0-rc.9",
    "nexus-prisma": "0.9.1",
    "prisma2": "2.0.0-preview021",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.3.7",
    "@types/node": "12.12.17",
    "@types/ws": "6.0.4",
    "now": "^17.0.3",
    "ts-node": "8.5.4",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.5",
    "yu-ncov-scrape-dxy": "^0.2.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT"
}
